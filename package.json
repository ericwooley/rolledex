{
  "name": "rolledex",
  "version": "0.0.0",
  "license": "MIT",
  "main": "apps/expo/AppEntry.js",
  "watch": {
    "auth-admin-gen": {
      "patterns": [
        "apps/auth/src",
        "metadata"
      ],
      "extensions": "gql,graphql",
      "runOnChangeOnly": true
    },
    "sdk-gen": {
      "patterns": [
        "libs/sdk/src/lib"
      ],
      "extensions": "gql,graphql",
      "runOnChangeOnly": true
    }
  },
  "scripts": {
    "nx": "nx",
    "dev": "docker-compose up -d && wait-on tcp:127.0.0.1:8080 && concurrently --names 'hasura,auth,expo,watch' -c 'bgBlue.bold,bgMagenta.bold,bgRed.bold,bgGreen.bold' 'sleep 2 && hasura console --no-browser' 'nx serve auth' 'nx serve expo' 'yarn watch'",
    "start": "nx serve",
    "build": "nx build",
    "test": "nx test",
    "generate": "gqless generate",
    "lint": "nx workspace-lint && nx lint",
    "e2e": "nx e2e",
    "affected:apps": "nx affected:apps",
    "affected:libs": "nx affected:libs",
    "affected:build": "nx affected:build",
    "affected:e2e": "nx affected:e2e",
    "affected:test": "nx affected:test",
    "affected:lint": "nx affected:lint",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected": "nx affected",
    "format": "nx format:write",
    "format:write": "nx format:write",
    "format:check": "nx format:check",
    "update": "nx migrate latest",
    "workspace-generator": "nx workspace-generator",
    "dep-graph": "nx dep-graph",
    "help": "nx help",
    "expo:start": "expo start",
    "expo:android": "expo start --android",
    "expo:ios": "expo start --ios",
    "expo:web": "expo start --web",
    "expo:eject": "expo eject",
    "postinstall": "typesync",
    "watch": "npm-watch",
    "auth-admin-gen": "graphql-codegen --config apps/auth/codegen.yml",
    "sdk-gen": "graphql-codegen --config libs/sdk/codegen.yml"
  },
  "private": true,
  "dependencies": {
